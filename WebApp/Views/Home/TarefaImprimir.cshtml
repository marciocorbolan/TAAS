@{
    Layout = "";
}

<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>TAAS - Teste de aptidão para Analista De Sistemas</title>
    <!-- Tell the browser to be responsive to screen width -->
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <environment include="Development">
        <!-- Font Awesome -->
        <link rel="stylesheet" href="~/lib/font-awesome/css/all.css">
        <!-- Ionicons -->
        <link rel="stylesheet" href="~/lib/ionicons/css/ionicons.css">
        <!-- Twitter Bootstrap -->
        <link rel="stylesheet" href="~/lib/twitter-bootstrap/css/bootstrap.css" />
        <!-- AdminLTE -->
        <link rel="stylesheet" href="~/lib/admin-lte/css/adminlte.css" />

        <!-- Google Font: Source Sans Pro -->
        <link href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,400i,700" rel="stylesheet">
    </environment>
    <environment exclude="Development">
        <!-- Font Awesome -->
        <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.2/css/all.min.css"
              asp-fallback-href="~/lib/font-awesome/css/all.min.css"
              asp-fallback-test-class="sr-only" asp-fallback-test-property="position" asp-fallback-test-value="absolute" />
        <!-- Ionicons -->
        <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/ionicons/2.0.1/css/ionicons.min.css"
              asp-fallback-href="~/lib/ionicons/css/ionicons.min.css"
              asp-fallback-test-class="sr-only" asp-fallback-test-property="position" asp-fallback-test-value="absolute" />
        <!-- Twitter Bootstrap -->
        <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/4.5.3/css/bootstrap.min.css"
              asp-fallback-href="~/lib/twitter-bootstrap/css/bootstrap.min.css"
              asp-fallback-test-class="sr-only" asp-fallback-test-property="position" asp-fallback-test-value="absolute" />
        <!-- AdminLTE -->
        <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/admin-lte/3.0.5/css/adminlte.min.css"
              asp-fallback-href="~/lib/admin-lte/css/adminlte.min.css"
              asp-fallback-test-class="sr-only" asp-fallback-test-property="position" asp-fallback-test-value="absolute" />

        <!-- Google Font: Source Sans Pro -->
        <link href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,400i,700" rel="stylesheet">
    </environment>
</head>
<body>
    <div class="wrapper">
        <!-- Main content -->
        <section class="tarefa">
            <!-- title row -->
            <div class="row">
                <div class="col-12 text-center">
                    <h2>
                        TESTE DE APTIDÃO PARA ANALISTA DE SISTEMAS
                    </h2>
                </div>
                <div class="col-12 text-right">
                    <small>Data: @DateTime.Now</small>
                    <hr />
                </div>
                <!-- /.col -->
            </div>
            <!-- /.row -->
            <!-- info row -->
            <div class="row tarefa-info">
                <div class="col-sm-12">
                    @await Html.PartialAsync("Tarefa")
                </div>
                <!-- /.col -->
            </div>
            <!-- /.row -->
        </section>
        <!-- /.content -->
    </div>
    <!-- ./wrapper -->

    <script type="text/javascript">
        window.addEventListener("load", window.print());
    </script>
</body>
</html>
